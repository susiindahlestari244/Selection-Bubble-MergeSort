//Selection_Sort
#include<iostream>
#include<cstdlib>
using namespace std;
int main(){
	int SelectionSort[]={3,5,7,9,1,8,6,4,2,10};
		cout<<"sebelum"<<endl;
	for (int i=0;i<10;i++){
		cout<<' '<<SelectionSort[i];
	}
	cout<<endl<<endl;
		for (int i=0;i<10;i++){
			
			int smallest=SelectionSort[i];
			int smallestindex=i;
			
			for(int j=i; j<10; j++){
				if(SelectionSort[j]<smallest){
					smallest=SelectionSort[j];
					smallestindex=j;
				}
			}
			
		swap(SelectionSort[i],SelectionSort[smallestindex]);	
		}
			cout<<"sesudah"<<endl;
			for (int i=0;i<10;i++){
			cout<<' '<<SelectionSort[i];
	}
	cout<<endl<<endl;
	return 0;
}

//Bubble_Sort
#include<iostream>
using namespace std;
int main(){
	int BubbleSort[]={98,95,43,56,78,21,2};
	cout<<"sebelum"<<endl;
	for (int i=0;i<6;i++){
		cout<<' '<<BubbleSort[i];
	}
	cout<<endl<<endl;
	for(int i=0;i<=6;i++){
		for(int j=i+1;j<=6;j++){
			int temp;
			if(BubbleSort[i]>BubbleSort[j]){
				temp=BubbleSort[i];
				BubbleSort[i]=BubbleSort[j];
				BubbleSort[j]=temp;
				
			}
		}
	}
		
		cout<<"sebelum"<<endl;
	for (int i=0;i<6;i++){
		cout<<' '<<BubbleSort[i];
		
}
return 0;

}


